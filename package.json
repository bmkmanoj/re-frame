{
  "name": "@re-frame/workspace",
  "private": true,
  "version": "0.1.4",
  "author": "David Zukowski <zukoweb@gmail.com> (https://zuko.me)",
  "description": "JavaScript port of Clojure's popular re-frame library",
  "workspaces": [
    "packages/*"
  ],
  "license": "MIT",
  "scripts": {
    "build": "lerna run build",
    "test": "ava"
  },
  "ava": {
    "files": [
      "packages/**/*.spec.js"
    ],
    "require": [
      "esm"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "ava": "^1.2.1",
    "esm": "^3.2.5",
    "lerna": "^3.13.0",
    "prettier": "^1.16.4",
    "rollup": "^1.2.2",
    "typescript": "^3.3.3"
  }
}
