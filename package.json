{
  "name": "@re-frame/workspace",
  "private": true,
  "version": "0.0.0",
  "author": "David Zukowski <zukoweb@gmail.com> (https://zuko.me)",
  "description": "JavaScript port of Clojure's popular re-frame library",
  "workspaces": [
    "packages/*"
  ],
  "license": "MIT",
  "scripts": {
    "test": "ava",
    "format": "prettier '**/*.js' --write"
  },
  "ava": {
    "files": [
      "packages/**/*.spec.js"
    ],
    "require": [
      "esm"
    ]
  },
  "prettier": {
    "semi": false,
    "bracketSpacing": false,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@types/node": "^11.10.5",
    "ava": "^1.2.1",
    "esm": "^3.2.5",
    "lerna": "^3.13.0",
    "prettier": "^1.16.4",
    "typescript": "^3.3.3"
  }
}
